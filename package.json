{
  "name": "tab-ast",
  "version": "1.0.0",
  "description": "codemirror 6 extension to generate an ast for music tablature",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "test": "jest"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stan15/tab-ast.git"
  },
  "keywords": [
    "parser",
    "music",
    "tabs",
    "guitar",
  ],
  "author": "Stanley Ihesiulo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Stan15/tab-ast/issues"
  },
  "homepage": "https://github.com/Stan15/tab-ast#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "rollup": "^2.69.0",
    "rollup-plugin-dts": "^4.1.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@codemirror/language": "^0.20.0",
    "@codemirror/state": "^0.20.0",
    "parser-tablature": "github:tab-edit/parser-tablature"
  }
